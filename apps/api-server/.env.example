# Database connection string
DATABASE_URL=postgresql://stock_user:stock_pass@localhost:5432/stock_db

# Price generator interval in milliseconds  
PRICE_INTERVAL_MS=10000

# Node environment
NODE_ENV=development

# Agent Application Base URL (used for CORS configuration)
AGENT_BASE_URL=http://localhost:3003

# Auth0 Configuration
# Your Auth0 domain (e.g., your-domain.auth0.com)
AUTH0_DOMAIN=your-domain.auth0.com

# Your Auth0 API Identifier/Audience (found in Auth0 Dashboard > APIs)
AUTH0_AUDIENCE=https://your-api-identifier
